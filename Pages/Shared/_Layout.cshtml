<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Test</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <script src="https://kit.fontawesome.com/13f4c7ca3f.js" crossorigin="anonymous"></script>
</head>
<body>
    <nav class="navbar fixed-top navbar-expand-lg navbar-dark pt-0 pb-1 pr-0 pl-0 ml-2">

        <span id="hamburger" class="nav-item pr-3 pl-2"><i class="fas fa-bars"></i></span>
        <a class="navbar-brand" href="#"> <img src="~/images/dim-logo.png" height="24" width="68" /> </a>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <div class="container-fluid">
                <div class="row">
                    <div class="col p-0 nav-start">
                        <ul class="navbar-nav">
                            <li class="nav-item active"><a class="nav-link" href="#">Inventory</a></li>
                            <li class="nav-item"><a class="nav-link" href="#">Progress</a></li>
                            <li class="nav-item"><a class="nav-link" href="#">Vendors</a></li>
                            <li class="nav-item"><a class="nav-link" href="#">Records</a></li>
                            <li class="nav-item"><a class="nav-link" href="#">Loadout Optimizer</a></li>
                            <li class="nav-item"><a class="nav-link" href="#">Organizer</a></li>
                        </ul>
                    </div>

                    <!--Search bar-->
                    <div class="col-7 p-0 nav-middle">
                        <form class="form-inline nav-item mt-1" action="#">
                            <i class="fas fa-search icon p-1"></i>
                            <input class="searchbox" type="text" id="searchText" name="searchBox" placeholder="Search item/perk, is:dupe, is:maxpower, -is:blue, and more" />
                            <span id="searchOption" class="option-icon p-1"><i class="fas fa-ellipsis-v"></i></span>
                            <span class="dropdown-icon"><i class="fas fa-chevron-down"></i></span>
                        </form>
                    </div>
                    <!--Sync settings icon-->
                    <span id="syncButton" class="nav-item"><i class="fas fa-sync"></i></span>
                    <span id="settingsButton" class="nav-item"><i class="fas fa-cog"></i></span>
                </div>
            </div>
        </div>
    </nav>
    <!--second nav-->
    <div class="container-fluid position-fixed char-banner">
